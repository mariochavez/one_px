<%= display_form_error_message photo %>

<%= form_for photo do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= display_field_error_message photo, :title %>
  </div>
  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
    <%= display_field_error_message photo, :description %>
  </div>
  <div>
    <%= f.label :tags %>
    <%= f.text_area :tags %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>

